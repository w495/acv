{
    type : "table",
    toolbar : [
        {type : "back"},
        
        {type : "button",
            name : "Обновить",
            action : "/get-all-acv-video-stats",
            icon : "icon/32/actions/edit-redo.png",
            specParam: "tab-change-state-action"}
            
    ],
    columns : [
        {name : "id", alias : "#", type : "float", sortable : true, width:"5%"},
        {name : "peer", alias : "ip", type : "string", sortable : true},
        {name : "datestart", alias : "Дата начала", type : "erl_datetime", sortable : true},
        {name : "datestop", alias : "Дата конца", type : "erl_datetime", sortable : true},
        {name : "click", alias : "Дата клика", type : "erl_datetime", sortable : true}
    ],

    filter : {
        fields : [
            {name : "fromdate", alias : "с даты", input : {type : "date-field"}},
            {name : "todate", alias : "по дату", input : {type : "date-field"}}
        ],
        render : "horisontal",
        submit_url : "/get-acv-video-stat/by-film"
    },
    
    index_name : ["id"],
    sort : "id",
    ascending : false,

    // dblclick_action : "resource/bsk/descr/acv-video-stat-by-film-tab.json"
}
