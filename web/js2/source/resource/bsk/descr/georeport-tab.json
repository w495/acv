{
    type : "table",
    toolbar : [
        {type : "button", name : "Показать график", action : "resource/bsk/descr/geo-report-chart.json", icon : "icon/32/apps/office-chart.png"}
    ],
    columns : [
        {name : "geoip", alias : "Страна", type : "string", sortable : true, width : "30%"},
        {name : "wtime", alias : "Хронометраж", type : "sec", sortable : true, width : 100},
        {name : "watches", alias : "Просмотров", type : "int", sortable : true, width : 90},
        {name : "avg_wtime", alias : "Ср. время просмотра", type: "sec", sortable : true},
        {name : "advshows", alias : "Показы рекламы", type : "int", sortable : true},
        {name : "advclicks", alias : "Clicks", type : "int", sortable : true},
        {name : "ctr", alias : "CTR, %", type : "int", sortable : true}
    ],

    filter : {
        fields : [
            {name : "fromdate", alias : "с даты", input : {type : "date-field"}},
            {name : "todate", alias : "по дату", input : {type : "date-field"}}
        ],
        buttons : [
    	    /*{alias : "отчет в csv", url : "/csv/stats-country"},
    	    {alias : "отчет в html", url : "/html/stats-country"}*/
         ],
        render : "horisontal",
        submit_url : "/stats-country"
    },

    sort : "country",
    ascending : false
    //,dblclick_action : "resource/bsk/descr/geoviewreport-tab.json"
}
